<%= form_for @doctor do |f| %>
  <%= f.label :first_name %><br />
  <%= f.text_field :first_name %><br />
  <br />
  <%= f.label :surname %><br />
  <%= f.text_field :surname %><br />
  <br />
  <%= f.label :phone_no %><br />
  <%= f.text_field :phone_no %><br />
  <br />
  <%= f.label :speciality %><br />
  <%= f.text_area :speciality %><br />
  <br />
  <%= f.submit %>
<% end %>
